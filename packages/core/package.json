  "license": "MIT",
  "dependencies": {
    "axios": "^0.27.2",
    "immer": "^9.0.7",
    "lodash": "^4.17.21",
    "marked": "^1.2.5",
    "moment": "^2.29.0",
    "qiankun": "^2.5.1",
    "query-string": "^6.13.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.178",
    "@types/marked": "^1.2.0",
    "@types/react": "^16.14.0"
  },
  "peerDependencies": {
    "react": "^16.14.0"
  }
}
