{
  "name": "ccms",
  "version": "1.4.0-beta.7",
  "description": "ConfigableCMS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "npm-watch build",
    "build": "npm run clear && npm run build:rollup",
    "build:rollup": "rollup -c",
    "build:babel": "babel src -d lib -s -x '.ts,.tsx'",
    "clear": "rm -rf dist && rm -rf lib",
    "test": "jest",
    "test:u": "npm test -- -u"
  },
  "keywords": [
    "ccms"
  ],
