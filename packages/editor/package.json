{
  "name": "ccms-editor",
  "version": "1.5.0",
  "description": "ccms编辑器",
  "main": "dist/index.js",
  "types": "lib/src/index.d.ts",
  "files": [
    "dist/*",
    "lib"
  ],
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev_bak": "webpack serve --config ./webpack.dev.js",
    "build": "webpack --config ./webpack.prod.js --mode production && node scripts/config.js",
    "clear": "rm -rf dist && rm -rf lib",
    "watch_config": "npm-watch run_config",
    "run_config": "node scripts/config.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "link": "npm link ccms ccms-antd-mini ccms-antd"
  },
  "watch": {
    "run_config": {
      "patterns": [
        "src/config"
      ],
      "extensions": "ts",
      "quite": false
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jd-orion/ccms/tree/main/packages/editor"
  },
  "author": "cui",
  "license": "MIT",
  "dependencies": {
    "@monaco-editor/react": "^4.3.1",
    "@types/react": "^17.0.2",
    "antd": "4.14.1",
    "axios": "^0.21.1",
    "copy-html-to-clipboard": "^4.0.1",
    "file-loader": "^6.2.0",
    "immer": "^9.0.7",
    "lodash": "^4.17.21",
    "marked": "^2.0.1",
    "qiankun": "^2.7.0",
    "query-string": "^7.0.0",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^15.0.0",
    "@types/react-dom": "^17.0.1",
    "@webpack-cli/serve": "^1.3.0",
    "css-loader": "^5.1.3",
    "express": "^4.17.1",
    "html-webpack-plugin": "^5.2.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "npm-watch": "^0.11.0",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^2.0.0",
    "ts-loader": "^9.3.0",
    "typescript": "^4.2.2",
    "webpack": "^5.24.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
