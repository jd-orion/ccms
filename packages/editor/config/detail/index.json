  {
    "field": "actions",
    "label": "操作特定参数",
    "primaryField": "actions",
    "type": "form",
    "canInsert": true,
    "canRemove": true,
    "canSort": true,
    "canCollapse": true,
    "fields": [
      {
        "field": "",
        "label": "",
        "type": "import_subform",
        "interface": {
          "url": "${configDomain}/detail/operation.json",
          "urlParams": [
            { "field": "version", "data": { "source": "source", "field": "version" } },
