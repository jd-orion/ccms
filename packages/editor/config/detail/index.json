      ],
      "method": "GET",
      "cache": { "global": "CCMS_CONFIG_common_ColumnsConfig" }
    },
    "condition": {
      "template": "${enable} === true && (${type} === 'group' || ${type} === 'import_subform')",
      "params": [
        { "field": "type", "data": { "source": "record", "field": "type" } },
        { "field": "enable", "data": { "source": "record", "field": "columns.enable" } }
